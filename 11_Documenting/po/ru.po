#: src/guesser.c:26
msgid "a simple program that guesses a number between 1 and 100"
msgstr "простая программа, угадывающая число от 1 до 100"

#: src/guesser.c:31
msgid "Use roman numbers instead of decimal"
msgstr "Использовать римские числа вместо десятичных"

#: src/guesser.c:337
#, c-format
msgid "Choose a number between %s and %s:\n"
msgstr "Загадайте число от %s до %s:\n"

#: src/guesser.c:343
#, c-format
msgid "Is chosen number more than %s? (%s/%s)\n"
msgstr "Загаданное число больше %s? (%s/%s)\n"

#: src/guesser.c:343 src/guesser.c:346
msgid "yes"
msgstr "да"

#: src/guesser.c:343 src/guesser.c:348
msgid "no"
msgstr "нет"

#: src/guesser.c:351
msgid "Unrecognized answer."
msgstr "Ответ не распознан."

#: src/guesser.c:355
#, c-format
msgid "Your chosen number is %s\n"
msgstr "Загаданное вами число - %s\n"
